{
  "name": "Ecos",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Gets information from the page.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/ecos16.png",
      "32": "images/ecos32.png",
      "48": "images/ecos48.png",
      "120": "images/ecos120.png"
    }
  },
  "icons": {
    "16": "images/ecos16.png",
    "32": "images/ecos32.png",
    "48": "images/ecos48.png",
    "120": "images/ecos120.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"]
}

